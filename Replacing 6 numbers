#include <stdio.h>
#include<conio.h>
void main()
{
	int a[10];
	int i;
  
	for(i=0; i<6; i++)
	{
		printf("%d. sayiyi giriniz:",i+1);
		scanf("%d",&a[i]);
	}
  
	for(i=0;  i<3; i++)
	{
    int temp=a[i];
    a[i]=a[5-i];
    a[5-i]=temp;
    }	
     
  for(i=0;  i<6; i++)
  {  
      printf("%d.sayi:%d\n",i+1,a[i]);
  }
}
